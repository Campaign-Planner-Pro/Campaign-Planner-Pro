<%= form_with model: @player_character, url: campaign_player_character_path(@campaign, @player_character), method: :patch do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name %>

  <%= f.label :background %>
  <%= f.text_area :background %>

  <%= f.submit data: {turbo_frame: "_top"} %>
<% end %>